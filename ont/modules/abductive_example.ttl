heals:MultipleDrugContraindication rdf:type owl:Class ;
    rdfs:subClassOf heals:MedicinalEffect ;
    rdfs:label "Multiple Drug Contraindication" .

heals:MultipleDrugContradictedPatient rdf:type owl:Class ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            fhir:Patient 
